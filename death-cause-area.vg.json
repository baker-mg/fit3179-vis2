{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "data/death_cause.csv"},
    "width": 300, "height": 200,
    "mark": "area",
    "encoding": {
      "x": {
        "field": "decade",
        "title": "Decade"
      },
      "y": {
        "aggregate": "sum", "field": "n",
        "stack": "normalize",
        "title": ""  
      },
      "color": {
          "field":"death_cause",
          "scale":{
              "scheme": "category20b"
            },
            "title": "Cause of Death"
        }
    }
  }